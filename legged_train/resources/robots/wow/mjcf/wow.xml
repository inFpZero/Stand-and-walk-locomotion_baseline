<mujoco model="wow">
    <compiler angle="radian" meshdir="../meshes_obj/" eulerseq="zyx"/>
    <option timestep='0.001' iterations='50' solver='PGS' gravity='0 0 -9.81'>
    <flag sensornoise="enable" frictionloss="enable"/>
    </option>
    <size njmax="500" nconmax="100" />

    <visual>
        <quality shadowsize='4096'/>
        <map znear='0.05'/>
    </visual>


        <default>
        <joint limited='true'/>
        <motor ctrllimited='true'/>
        <geom  condim='4' contype="1" conaffinity="15" solref='0.001 2' friction='0.9 0.2 0.2'/>
        <equality solref='0.001 2'/>
        <default class='visualgeom'>
            <geom material='visualgeom' condim='1' contype='0' conaffinity='0'/>
        </default>
        <default class='visualgeom2'>
            <geom material='visualgeom2' condim='1' contype='0' conaffinity='0'/>
        </default>
        <default class='obstacle'>
            <geom material='obstacle' condim='3' contype='1' conaffinity='15'/>
        </default>

        <default class='leg_joint_param'>
            <joint  damping="0.01" frictionloss="0.01" armature="0.01"/>
        </default>

    </default>


    <asset>

        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="512"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2"
            width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>
        <texture name="texplane2" type="2d" builtin="checker" rgb1="1 0.3137 0.1843" rgb2="0.0 0.30196 0.38039"
            width="512" height="512" mark="cross" markrgb=".8 .8 .8"/>

        <material name="matplane" reflectance="0." texture="texplane" texrepeat="1 1" texuniform="true"/>
        <material name="matplane2" reflectance="0.1" texture="texplane2" texrepeat="1 1" texuniform="true"/>

        <material name='obstacle'  rgba='0.9 0.6 0.2 1'/>
        <material name='visualgeom'  rgba='0.5 0.9 0.2 1'/>
        <material name='visualgeom2'  rgba='0.5 0.9 0.2 1'/>

        <mesh name="body" file="body.obj" />
        <mesh name="left_roll_Link" file="left_roll_Link.obj" />
        <mesh name="left_yaw_Link" file="left_yaw_Link.obj" />
        <mesh name="left_pitch_Link" file="left_pitch_Link.obj" />
        <mesh name="left_knee_Link" file="left_knee_Link.obj" />
        <mesh name="left_foot_Link" file="left_foot_Link.obj" />
        <mesh name="right_roll_Link" file="right_roll_Link.obj" />
        <mesh name="right_yaw_Link" file="right_yaw_Link.obj" />
        <mesh name="right_pitch_Link" file="right_pitch_Link.obj" />
        <mesh name="right_knee_Link" file="right_knee_Link.obj" />
        <mesh name="right_foot_Link" file="right_foot_Link.obj" />

        <hfield name='hf0' nrow='200' ncol='200' size="10 5 0.2 .1"/>
    </asset>
    <worldbody>

        <light directional="true" diffuse=".4 .4 .4" specular="0.1 0.1 0.1" pos="0 0 5.0" dir="0 0 -1" castshadow="false"/>
        <light directional="true" diffuse=".6 .6 .6" specular="0.2 0.2 0.2" pos="0 0 4" dir="0 0 -1"/>
        <geom name="ground" type="plane" size="0 0 1" pos="0.001 0 0" quat="1 0 0 0" material="matplane" condim="1" conaffinity='15'/>

        <body name="body" pos="0.0 0.0 0.912">
            <!-- <freejoint name ="root"/> -->
            <geom type="mesh" mesh="body" group="1" class="visualgeom"/>
            <inertial pos="-0.0062523 0.00010748 -0.039166" mass="8.46" diaginertia="0.26319860 0.14931203 0.14097779" />
            <joint type='slide' axis='1 0 0' limited='false' />
            <joint type='slide' axis='0 1 0' limited='false' />
            <joint type='slide' axis='0 0 1' limited='false' />
            <joint type='ball' limited='false' />
            <site name='imu' size='0.01' pos='0.0 0 0.0'/>
        
            <body name="left_roll_Link" pos="-0.05 0.075 -0.2289">
                <inertial pos="0.047592 -0.0022216 0.0025354" quat="0.0779809 0.695761 -0.0877911 0.70861" mass="0.705" diaginertia="0.00053124 0.00057449 0.00091320" />
                <joint name="left_roll_joint" pos="0 0 0 " axis="1 0 0" limited="true" range="-0.2 1.5" />
                <geom type="mesh" rgba="1 1 1 1" mesh="left_roll_Link" />
                <body name="left_yaw_Link" pos="0.052 0 -0.01735">
                    <inertial pos="-5.4658e-09 -0.019246 -0.027515" quat="0.689309 -0.157647 0.157647 0.689309" mass="0.097" diaginertia="0.00007712 0.00006715 0.00003376" />
                    <joint name="left_yaw_joint" pos="0 0 0 " axis="0 0 -1" limited="true" range="-0.7 0.7" />
                    <geom type="mesh" rgba="1 1 1 1" mesh="left_yaw_Link" />
                    <body name="left_pitch_Link" pos="0 -0.025 -0.0635">
                        <inertial pos="4.286e-05 0.028462 -0.080837" quat="0.687951 -0.00130246 0.0149468 0.725602" mass="2.72119" diaginertia="0.01937616 0.02055257 0.00236629" />
                        <joint name="left_pitch_joint" pos="0 0 0" axis="0 1 0" limited="true" range="-1.57 0.5" />
                        <geom type="mesh" rgba="1 1 1 1" mesh="left_pitch_Link" />
                        <body name="left_knee_Link" pos="0 0.0185 -0.275">
                            <inertial pos="-0.00042722 0.0043352 -0.080717" quat="0.518444 0.00626424 0.0187855 0.854882" mass="0.965" diaginertia="0.00462805 0.00472890 0.00045533" />
                            <joint name="left_knee_joint" pos="0 0 0" axis="0 1 0" limited="true" range="0 1.57" />
                            <geom type="mesh" rgba="1 1 1 1" mesh="left_knee_Link" />
                            <body name="left_foot_Link" pos="0.0084516 0.002 -0.27487">
                                <inertial pos="0.0089522 0.004767 -0.034577" quat="0.480126 0.516419 0.521169 0.480804" mass="0.12731" diaginertia="0.00003267 0.00023186 0.00020486" />
                                <joint name="left_foot_joint" pos="0 0 0" axis="0 1 0" limited="true" range="-0.6 0.6" />
                                <geom type="mesh" rgba="1 1 1 1" mesh="left_foot_Link" />
                            </body>
                        </body>
                    </body>
                </body>
            </body>
            <body name="right_roll_Link" pos="-0.05 -0.075 -0.2289">
                <inertial pos="0.047589 0.002217 0.0025116" quat="-0.0777388 0.695136 0.0877543 0.709255" mass="0.705" diaginertia="0.00053146 0.00057446 0.00091320" />
                <joint name="right_roll_joint" pos="0 0 0" axis="1 0 0" limited="true" range="-1.57 0.2" />
                <geom type="mesh" rgba="1 1 1 1" mesh="right_roll_Link" />
                <body name="right_yaw_Link" pos="0.052 0 -0.01735">
                    <inertial pos="5.4658e-09 0.019246 -0.027515" quat="0.689309 0.157647 -0.157647 0.689309" mass="0.097" diaginertia="0.00007712 0.00006715 0.00003376" />
                    <joint name="right_yaw_joint" pos="0 0 0" axis="0 0 -1" limited="true" range="-0.7 0.7" />
                    <geom type="mesh" rgba="1 1 1 1" mesh="right_yaw_Link" />
                    <body name="right_pitch_Link" pos="0 0.025 -0.0635">
                        <inertial pos="2.2137e-05 -0.028465 -0.080779" quat="0.729793 -0.00227177 0.0147077 0.683506" mass="2.72119" diaginertia="0.01938088 0.02055794 0.00236723" />
                        <joint name="right_pitch_joint" pos="0 0 0" axis="0 -1 0" limited="true" range="-1.57 0.5" />
                        <geom type="mesh" rgba="1 1 1 1" mesh="right_pitch_Link" />
                        <body name="right_knee_Link" pos="0 -0.0185 -0.275">
                            <inertial pos="-0.00042634 -0.0043353 -0.080717" quat="0.85577 0.0187791 0.00639247 0.516975" mass="0.965" diaginertia="0.00466994 0.00476936 0.00045693" />
                            <joint name="right_knee_joint" pos="0 0 0" axis="0 -1 0" limited="true" range="-1.57 0" />
                            <geom type="mesh" rgba="1 1 1 1" mesh="right_knee_Link" />
                            <body name="right_foot_Link" pos="0.0084515 -0.002 -0.27487">
                                <inertial pos="0.0089522 -0.004767 -0.034577" quat="0.480804 0.521169 0.516419 0.480126" mass="0.12731" diaginertia="0.00003267 0.00023186 0.00020486" />
                                <joint name="right_foot_joint" pos="0 0 0" axis="0 -1 0" limited="true" range="-0.6 0.6" />
                                <geom type="mesh" rgba="1 1 1 1" mesh="right_foot_Link" />
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>    
    </worldbody>
        
    <actuator>
        <motor name="left_roll_joint" joint="left_roll_joint" gear="1" ctrllimited="true" ctrlrange="-36 36"/>
        <motor name="left_yaw_joint" joint="left_yaw_joint" gear="1" ctrllimited="true" ctrlrange="-36 36"/>
        <motor name="left_pitch_joint" joint="left_pitch_joint" gear="1" ctrllimited="true" ctrlrange="-58 58"/>
        <motor name="left_knee_joint" joint="left_knee_joint" gear="1" ctrllimited="true" ctrlrange="-58 58"/>
        <motor name="left_foot_joint" joint="left_foot_joint" gear="1" ctrllimited="true" ctrlrange="-36 36"/>

        <motor name="right_roll_joint" joint="right_roll_joint" gear="1" ctrllimited="true" ctrlrange="-36 36"/>
        <motor name="right_yaw_joint" joint="right_yaw_joint" gear="1" ctrllimited="true" ctrlrange="-36 36"/>
        <motor name="right_pitch_joint" joint="right_pitch_joint" gear="1" ctrllimited="true" ctrlrange="-58 58"/>
        <motor name="right_knee_joint" joint="right_knee_joint" gear="1" ctrllimited="true" ctrlrange="-58 58"/>
        <motor name="right_foot_joint" joint="right_foot_joint" gear="1" ctrllimited="true" ctrlrange="-36 36"/>

    </actuator>

        <sensor>
            <actuatorpos name='left_roll_joint_p'  actuator='left_roll_joint' user='13'/>
            <actuatorpos name='left_yaw_joint_p'  actuator='left_yaw_joint' user='13'/>
            <actuatorpos name='left_pitch_joint_p'  actuator='left_pitch_joint' user='13'/>
            <actuatorpos name='left_knee_joint_p'  actuator='left_knee_joint' user='13'/>
            <actuatorpos name='left_foot_joint_p'  actuator='left_foot_joint' user='13'/>

            <actuatorpos name='right_roll_joint_p'  actuator='right_roll_joint' user='13'/>
            <actuatorpos name='right_yaw_joint_p'  actuator='right_yaw_joint' user='13'/>
            <actuatorpos name='right_pitch_joint_p'  actuator='right_pitch_joint' user='13'/>
            <actuatorpos name='right_knee_joint_p'  actuator='right_knee_joint' user='13'/>
            <actuatorpos name='right_foot_joint_p'  actuator='right_foot_joint' user='13'/>


            <actuatorvel name='left_roll_joint_v'  actuator='left_roll_joint' user='13'/>
            <actuatorvel name='left_yaw_joint_v'  actuator='left_yaw_joint' user='13'/>
            <actuatorvel name='left_pitch_joint_v'  actuator='left_pitch_joint' user='13'/>
            <actuatorvel name='left_knee_joint_v'  actuator='left_knee_joint' user='13'/>
            <actuatorvel name='left_foot_joint_v'  actuator='left_foot_joint' user='13'/>

            <actuatorvel name='right_roll_joint_v'  actuator='right_roll_joint' user='13'/>
            <actuatorvel name='right_yaw_joint_v'  actuator='right_yaw_joint' user='13'/>
            <actuatorvel name='right_pitch_joint_v'  actuator='right_pitch_joint' user='13'/>
            <actuatorvel name='right_knee_joint_v'  actuator='right_knee_joint' user='13'/>
            <actuatorvel name='right_foot_joint_v'  actuator='right_foot_joint' user='13'/>

            <actuatorfrc name='left_roll_joint_f'  actuator='left_roll_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='left_yaw_joint_f'  actuator='left_yaw_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='left_pitch_joint_f'  actuator='left_pitch_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='left_knee_joint_f'  actuator='left_knee_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='left_foot_joint_f'  actuator='left_foot_joint' user='13' noise='1e-3'/>

            <actuatorfrc name='right_roll_joint_f'  actuator='right_roll_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='right_yaw_joint_f'  actuator='right_yaw_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='right_pitch_joint_f'  actuator='right_pitch_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='right_knee_joint_f'  actuator='right_knee_joint' user='13' noise='1e-3'/>
            <actuatorfrc name='right_foot_joint_f'  actuator='right_foot_joint' user='13' noise='1e-3'/>


            <framequat     name='orientation' objtype='site' noise='0.001' objname='imu'/>
            <framepos     name='position' objtype='site' noise='0.001' objname='imu'/>
            <gyro          name='angular-velocity'    site='imu' noise='0.005' cutoff='34.9'/>
            <velocimeter   name='linear-velocity'     site='imu' noise='0.001' cutoff='30'/>
            <accelerometer name='linear-acceleration' site='imu' noise='0.005' cutoff='157'/>
            <magnetometer  name='magnetometer'        site='imu'/>

    </sensor>
</mujoco>
